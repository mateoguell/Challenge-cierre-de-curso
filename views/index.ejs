<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Listado de películas</title>
</head>

<body>
  <table>
    <thead>
      <tr>
        <th>
          <h1>Listado de películas</h1>
          <h3><a href="/createP">Agregar película</a></h3>
        </th>
      </tr>
    </thead>
    <tbody>
      <% movies.forEach(function(movie) { %>
        <tr>
          <td><a href="/peliculasD/<%= movie.id %>">
              <%= movie.title %>
            </a></td>
        </tr>
        <% }); %>
    </tbody>
  </table>
</body>

</html>